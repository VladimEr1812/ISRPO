{
  "name": "case-converter",
  "displayName": "Case Converter",
  "description": "A VS Code extension to convert text to various cases (UPPER, lower, camel, snake, kebab).",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
        {
            "command": "case-converter.toUppercase",
            "title": "Case Converter: to Upper case"
        },
        {
            "command": "case-converter.toLowercase",
            "title": "Case Converter: to lower case"
        },
        {
            "command": "case-converter.toCamelCase",
            "title": "Case Converter: to camelCase"
        },
        {
            "command": "case-converter.toSnakeCase",
            "title": "Case Converter: to snake_case"
        },
        {
            "command": "case-converter.toKebabCase",
            "title": "Case Converter: to kebab-case"
        }
    ]
},
"activationEvents": [
    "onCommand:case-converter.toUppercase",
    "onCommand:case-converter.toLowercase",
    "onCommand:case-converter.toCamelCase",
    "onCommand:case-converter.toSnakeCase",
    "onCommand:case-converter.toKebabCase"
],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.100.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
